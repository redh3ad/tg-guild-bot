{
  "compilerOptions": {
    "target": "ES2019",
    "module": "commonjs",
    // "rootDir": "./api",
    // "rootDir": "src",
    "baseUrl": ".",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "outDir": "./build",
    // "outDir": "dist",
    "allowSyntheticDefaultImports": true,
    "forceConsistentCasingInFileNames": true,
    "esModuleInterop": true,
    "strict": true,
    "skipLibCheck": true
  },
  "exclude": ["node_modules"]
}
